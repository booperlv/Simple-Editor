{
   "app": {
      "background": {
         "persistent": false,
         "scripts": [ "js/background.js", "js/web-server-chrome/js/encoding.js", "js/web-server-chrome/js/underscore.js", "js/web-server-chrome/js/common.js", "js/web-server-chrome/js/mime.js", "js/web-server-chrome/js/buffer.js", "js/web-server-chrome/js/request.js", "js/web-server-chrome/js/stream.js", "js/web-server-chrome/js/connection.js", "js/web-server-chrome/js/webapp.js", "js/web-server-chrome/js/handlers.js", "js/web-server-chrome/js/httplib.js", "js/web-server-chrome/js/background.js" ]
      }
   },
   "description": "Simple Editor",
   "file_handlers": {
      "text": {
         "extensions": [ "coffee", "css", "dart", "go", "js", "json", "jsx", "less", "markdown", "md", "php", "py", "rb", "sass", "scss", "svg", "txt", "vbs", "java" ],
         "types": [ "application/javascript", "application/json", "application/x-shellscript", "text/*" ]
      }
   },
   "icons": {
      "128": "img/icon128.png",
      "16": "img/icon16.png",
      "32": "img/icon32.png",
      "48": "img/icon48.png"
   },
   "manifest_version": 2,
   "minimum_chrome_version": "42",
   "name": "Simple Editor",
   "offline_enabled": true,
   "permissions": [ "syncFileSystem", "browser", "storage", "unlimitedStorage", "webview", "clipboardRead", "clipboardWrite", "*://github.com/*", "*://*.github.com/*", {
      "fileSystem": [ "write", "directory", "retainEntries" ]
   } ],
   "sockets": {
      "tcp": {
         "connect": [ "" ]
      },
      "tcpServer": {
         "listen": [ "127.0.0.1:*" ]
      },
      "udp": {
         "bind": [ "" ],
         "send": [ "" ]
      }
   },
   "version": "7.1",
   "webview": {
      "partitions": [ {
         "accessible_resources": [ "*.*" ],
         "name": "html5-webview"
      } ]
   }
}
